{
	"extends": ["airbnb", "airbnb/hooks"],
	"env": {
	  "browser": true,
	  "node": true,
	  "es2021": true
	},
	"parserOptions": {
	  "ecmaVersion": 12,
	  "sourceType": "module"
	},
	"rules": {
	  // Customize or override Airbnb rules
	  "no-console": "off",
	  "linebreak-style": 0,
	  "semi":["error","never"],
	  "indent": ["error", "tab"],
	  "no-tabs":"off"
	}
  }